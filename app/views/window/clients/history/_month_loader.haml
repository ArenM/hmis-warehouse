= content_for :page_js do
  :javascript
    (function($) {
      queue_name = 'get_months';
      months = #{@client.months_served.reverse.to_json}
      $.each(months, function(m) {
        $(document).queue(queue_name, function() {
          $.get('#{polymorphic_path(month_of_service_path_generator, client_id: @client.id)}', months[m], function(data) {
            $('#client-history').append(data);
            $(document).dequeue(queue_name);  
          });
        });
      });
      $(document).dequeue(queue_name);
    })(jQuery);