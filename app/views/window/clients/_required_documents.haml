- required_documents = GrdaWarehouse::Config.get(:document_ready)
- if required_documents.any?
  %h3 Document Ready?
  %table.table.table-condensed
    %thead
      %tr
        %th File
        %th Uploaded
    %tbody
      - @client.document_readiness(required_documents).each do |file|
        %tr
          %td
            = file.name
          %td
            - if file.available
              = checkmark('Y')
              = file.updated_at&.to_date