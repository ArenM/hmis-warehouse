- id = 'triage-info'
#triage-accordion.panel-group.panel-collapsible
  - content_for :panel_collapse_content, flush: true do
    = render "he/clients/#{health_emergency}/triage_form"
  - content_for "panel_collapse_heading_content_#{id}" do
    = render "he/clients/#{health_emergency}/in_header_status", labels: ['Triage', @client.health_emergency_triage_status]
  = render 'common/panel_collapse', id: id, title: 'Triage'
