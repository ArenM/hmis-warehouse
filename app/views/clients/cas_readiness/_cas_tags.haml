- if @client.cas_tags.values.any?(&:present?)
  %section.o-section-card
    %header
      %h3 CAS Tags
    .c-card.c-card--flush.c-card--padded.c-card--block
      %table.table
        %thead
          %tr
            %th Tag
            %th Rank
        %tbody
          - @client.cas_tags.each do |id, rank|
            - tag = Cas::Tag.find(id)
            %tr
              %td= tag&.name
              %td= rank