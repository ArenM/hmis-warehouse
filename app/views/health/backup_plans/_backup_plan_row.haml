%tr{data: {id: backup_plan.id}}
  %td= backup_plan.description
  %td= backup_plan.backup_plan
  %td= backup_plan.person
  %td= backup_plan.phone
  %td= backup_plan.address
  %td= backup_plan.plan_created_on
  %td.text-right
    = link_to polymorphic_path([:edit] + health_path_generator + [:backup_plan], id: backup_plan.id), data: { loads_in_pjax_modal: true }, class: 'btn btn-secondary btn-sm btn-icon-left' do
      %i.icon-pencil
      Edit