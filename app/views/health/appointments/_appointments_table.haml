%table.table.table-condensed
  %thead
    %tr
      %th Date
      %th Department
      %th Type
      %th Doctor
      %th SA
  %tbody
    - appointments.each do |visit|
      %tr
        %td= visit.appointment_time
        %td= visit.department
        %td= visit.appointment_type
        %td= visit.doctor
        %td= visit.sa