- title = 'Collaborative Care Plan'
= content_for(:title, title)
%h1= "#{content_for(:title)} for #{@client.full_name}"
= render 'clients/breadcrumbs'
= render 'clients/tab_navigation', current: polymorphic_path(careplans_path_generator)
= render 'window/health/patient/patient_status'

.row
  .col-sm-4
    - unless @readonly
      = render 'assessments'
  .col-sm-8
    = render 'summary'
    = link_to polymorphic_path(careplan_path_generator + [:team, :members]) do
      Placeholder for access to the Team
    = render 'epic_goals'
