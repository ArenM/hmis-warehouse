- data = @data.send(section)
- if data.present?
  %h4= title
  = render 'bar_from_table', section: section
  %table.table.table-striped{id: "#{section}_table"}
    %thead
      %tr
        %th Project
        - GrdaWarehouse::Hud::Client.extended_age_groups.each do |_, age_bucket|
          %th= age_bucket[:name]
    %tbody
      - @data.involved_projects.sort_by(&:last).each do |(p_id, p_name)|
        %tr
          %th= p_name
          - GrdaWarehouse::Hud::Client.extended_age_groups.each do |_, age_bucket|
            %td
              - key = "#{p_id}__#{age_bucket[:name].parameterize.underscore}"
              - count = data[key] || 0
              = link_to_if(count > 0 && can_view_all_reports?, count, warehouse_report_support_index_path(@report.id, key: key, section: section, title: "#{section.to_s.titleize} for: #{p_name}â€”#{age_bucket[:name]}"), data: { loads_in_pjax_modal: true })