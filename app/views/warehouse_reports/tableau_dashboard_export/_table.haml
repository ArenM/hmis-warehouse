%p= paginate @reports
.table-responsive
  %table.table.table-condensed
    %thead
      %th CoC Code
      %th User
      %th Status
      %th Started
      %th Finished
      %th Download
      %th Delete
    %tbody
      - @reports.each do |report|
        %tr
          %td= report.display_coc_code
          %td= report.user_name
          %td= report.status
          %td= report.created_at
          %td= report.completed_at
          %td
            - if report.completed_at
              = link_to action: :show, id: report.id do
                %span.icon-download2
                Download
          %td
            - if report.completed_at
              = link_to warehouse_reports_tableau_dashboard_export_path(report), method: :delete, class: 'btn btn-sm btn-danger btn-icon-only' do
                %i.icon-cross