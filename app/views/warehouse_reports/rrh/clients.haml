= content_for :modal_title do
  - title = "Supporting Data"
  - if params[:selected_project].present?
    - title += " for #{@report.valid_project_name(params[:selected_project])}"
- if @clients.any?
  %table.table.table-sm
    %thead
      %tr
        %th Warehouse ID
        %th First Name
        %th Last Name
    %tbody
      - @clients.each do |id, first_name, last_name|
        - if current_user.can_view_clients? || current_user.can_edit_clients?
          - url = client_path(id: id)
        - elsif current_user.can_view_client_window?
          - url = window_client_path(id: id)
        %tr
          %td= link_to_if url.present?, id, url, target: '_blank'
          %td= link_to_if url.present?, first_name, url, target: '_blank'
          %td= link_to_if url.present?, last_name, url, target: '_blank'
