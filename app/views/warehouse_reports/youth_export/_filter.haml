= simple_form_for @filter, as: :filter, url: warehouse_reports_youth_export_index_path, html: { method: :get, class: 'jFilter' } do |f|
  = f.error_notification
  .row
    .col-sm-4
      = f.input :start, input_html: {value: @filter.start, class: 'form-control jStart'}, as: :date_picker, label: false
    .col-sm-4
      = f.input :end, input_html: {value: @filter.end, class: 'form-control jStart'}, as: :date_picker, label: false
    .col-sm-4
      %div.pull-right
        = f.button :submit, value: 'Update Range'
%hr