
wb = xlsx_package.workbook
wb.add_worksheet(name: "Open Enrollments") do |sheet|
  title = sheet.styles.add_style(sz: 12, b: true, alignment: {horizontal: :center})
  sheet.add_row([
    'Warehouse Client ID',
    'First Name', 
    'Last Name', 
    'Project', 
    'Entry Date', 
    'Most Recent Date Served',
  ], :style => title)
  @entries.each do |entry|
    client = @clients[entry[:client_id]]
    max_date = @max_dates[[entry[:data_source_id], entry[:project_id], entry[:enrollment_group_id], entry[:project_name], entry[:client_id]]]
    sheet.add_row([
      client[:id], 
      client[:FirstName], 
      client[:LastName], 
      entry[:project_name],
      entry[:first_date_in_program],
      max_date,
    ])
  end
end