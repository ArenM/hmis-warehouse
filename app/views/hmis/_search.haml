.o-page__search
  = search_form_for @search, url: hmis_path, method: :get do |f|
    .input-group
      = f.search_field :name_cont, label: false, placeholder: 'Search by id', class: 'form-control'
      = f.input :type, collection: GrdaWarehouse::Hud.hud_csv_names
      %span.input-group-btn
        %button.btn.btn-secondary{type: :submit} Search