.well.overflow-scroll
	.row
		.col-sm-6
			%h3.text-center Time to Enter
			.col-sm-6
				%h4.text-center Goal
				%hr
				%h3.text-center 14 days or less
			.col-sm-6
				%h4.text-center This Program
				%hr
				%h3.text-center
					= "#{@report.report['average_timeliness_of_entry']['Average']} days"
			.time-to-enter-chart.jChart
				%p.jLoading Loading
		.col-sm-6
			%h3.text-center Time to Exit
			.col-sm-6
				%h4.text-center Goal
				%hr
				%h3.text-center 14 days or less
			.col-sm-6
				%h4.text-center This Program
				%hr
				%h3.text-center
					= "#{@report.report['average_timeliness_of_exit']['Average']} days"
			.time-to-exit-chart.jChart
				%p.jLoading Loading