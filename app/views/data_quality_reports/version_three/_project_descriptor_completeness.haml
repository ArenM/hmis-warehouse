.well.overflow-scroll
  %h3.text-center Project Descriptor Completeness
  %br
  .row
    .col-sm-3
      %h4.text-center
        Bed Inventory
      %p.text-center
        = "#{@report.report['bed_utilization_totals']['counts']['capacity']} beds"
    .col-sm-3
      %h4.text-center
        CoC Code
      %p.text-center
        = @report.report['coc_code']
    .col-sm-3
      %h4.text-center
        Funder
      %p.text-center
        = @report.report['grant_id']
    .col-sm-3
      %h4.text-center
        Geocode
      %p.text-center
        = @report.report['geocode']
  .row
    .col-sm-3
      %h4.text-center
        Geography Type
      %p.text-center
        = @report.report['geography_type']
    .col-sm-3
      %h4.text-center
        Housing Type
      %p.text-center
        = @report.report['housing_type']
    .col-sm-3
      %h4.text-center
        Information Date
      %p.text-center
        = @report.report['information_date']
    .col-sm-3
      %h4.text-center
        Operating Start Date
      %p.text-center
        = @report.report['operating_start_date']
  .row
    .col-sm-3
      %h4.text-center
        Project Type
      %p.text-center
        = @report.report['coc_program_component']
    .col-sm-3
      %h4.text-center
        Unit Inventory
      %p.text-center
        = "#{@report.report['unit_utilization_totals']['counts']['capacity']} units"
