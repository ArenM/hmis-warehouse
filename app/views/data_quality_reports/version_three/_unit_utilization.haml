.well.overflow-scroll
	.row
		.col-sm-4
			%h4.title Unit Inventory
			%hr
			%h3.text-center
				= "#{@report.report['unit_utilization_totals']['counts']['capacity']} units"
			%p
			%h4.title Average Daily Households
			%hr
			%h3.text-center
				= "#{@report.report['unit_utilization_totals']['counts']['average_daily']} units"
			%p
			%h4.title Average Unit Utilization
			%hr
			%h3.text-center
				= "#{@report.report['unit_utilization_totals']['counts']['average_daily_percentage']}%"

		.col-sm-8
			%h3 Nightly Unit Census
			.unit-census-chart.jChart
				%p.jLoading Loading